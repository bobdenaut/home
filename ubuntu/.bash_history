#!/bin/bash
# netmon.sh - Monitorizare trafic network în timp real
INTERFACES=$(ls /sys/class/net | grep -E 'enp|wlo')  # alege interfețele de interes
echo "Monitoring interfaces: $INTERFACES"
echo "Press Ctrl+C to stop."
PREV_RX=()
PREV_TX=()
# initialize arrays
for IF in $INTERFACES; do     RX=$(cat /sys/class/net/$IF/statistics/rx_bytes);     TX=$(cat /sys/class/net/$IF/statistics/tx_bytes);     PREV_RX+=($RX);     PREV_TX+=($TX); done
while true; do     sleep 1;     i=0;     echo "---------------------------";     for IF in $INTERFACES; do         RX_NOW=$(cat /sys/class/net/$IF/statistics/rx_bytes);         TX_NOW=$(cat /sys/class/net/$IF/statistics/tx_bytes);          RX_BPS=$(( (RX_NOW - PREV_RX[i]) * 8 / 1000000 ))  # Mbps
        TX_BPS=$(( (TX_NOW - PREV_TX[i]) * 8 / 1000000 ))  # Mbps
         echo "$IF: Download: $RX_BPS Mbps | Upload: $TX_BPS Mbps";          PREV_RX[i]=$RX_NOW;         PREV_TX[i]=$TX_NOW;         ((i++));     done; done
clear
#!/bin/bash
# netmon_avg.sh - Monitorizare trafic network în timp real cu medie 5 secunde
INTERFACES=$(ls /sys/class/net | grep -E 'enp|wlo')  # interfețe de interes
SLEEP_INTERVAL=1
AVG_INTERVAL=5
echo "Monitoring interfaces: $INTERFACES"
echo "Press Ctrl+C to stop."
declare -A RX_PREV
declare -A TX_PREV
declare -A RX_SUM
declare -A TX_SUM
declare -A COUNTER
# Initialize
for IF in $INTERFACES; do     RX_PREV[$IF]=$(cat /sys/class/net/$IF/statistics/rx_bytes);     TX_PREV[$IF]=$(cat /sys/class/net/$IF/statistics/tx_bytes);     RX_SUM[$IF]=0;     TX_SUM[$IF]=0;     COUNTER[$IF]=0; done
while true; do     sleep $SLEEP_INTERVAL;     for IF in $INTERFACES; do         RX_NOW=$(cat /sys/class/net/$IF/statistics/rx_bytes);         TX_NOW=$(cat /sys/class/net/$IF/statistics/tx_bytes);          RX_DIFF=$((RX_NOW - RX_PREV[$IF]));         TX_DIFF=$((TX_NOW - TX_PREV[$IF]));          RX_SUM[$IF]=$((RX_SUM[$IF] + RX_DIFF));         TX_SUM[$IF]=$((TX_SUM[$IF] + TX_DIFF));         COUNTER[$IF]=$((COUNTER[$IF] + 1));          RX_PREV[$IF]=$RX_NOW;         TX_PREV[$IF]=$TX_NOW;          if [ ${COUNTER[$IF]} -eq $AVG_INTERVAL ]; then             RX_Mbps=$(( RX_SUM[$IF]*8 / (1000000*AVG_INTERVAL) ));             TX_Mbps=$(( TX_SUM[$IF]*8 / (1000000*AVG_INTERVAL) ));             echo "$IF: Download: $RX_Mbps Mbps | Upload: $TX_Mbps Mbps";             RX_SUM[$IF]=0;             TX_SUM[$IF]=0;             COUNTER[$IF]=0;         fi;     done; done
