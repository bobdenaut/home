#!/bin/bash
# Launcher Ungoogled Chromium AppImage cu GPU pe Wayland/KDE
APPIMAGE="$HOME/Downloads/ungoogled-chromium-140.0.7339.127-1-x86_64.AppImage"
# Asigură user namespaces pentru sandbox
sudo sysctl -w kernel.unprivileged_userns_clone=1
# Variabile mediu pentru GPU și Wayland
export LIBVA_DRIVER_NAME=nvidia
export VDPAU_DRIVER=nvidia
export XDG_SESSION_TYPE=wayland
export DISPLAY=:0
