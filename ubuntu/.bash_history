INTERVAL=5  # setează aici intervalul în secunde
while true; do   clear;   TEMP=$(</sys/class/thermal/thermal_zone0/temp);   TEMP_C=$((TEMP / 1000));    CPU_LOAD=$(top -bn1 | grep "Cpu(s)" | awk '{print 100 - $8"%"}'); 
  MEM_TOTAL=$(free -m | awk '/Mem:/ {print $2}');   MEM_USED=$(free -m | awk '/Mem:/ {print $3}');   MEM_PERCENT=$(awk "BEGIN {printf \"%.1f\", ($MEM_USED/$MEM_TOTAL)*100}"); 
  echo "$MEM_USED" >> /tmp/mem_history.txt;   if [ $(wc -l < /tmp/mem_history.txt) -gt 30 ]; then     sed -i '1d' /tmp/mem_history.txt;   fi;    MEM_MAX=$(awk 'BEGIN{max=0}{if($1>max)max=$1}END{print max}' /tmp/mem_history.txt);   MEM_AVG=$(awk '{sum+=$1} END {if (NR > 0) printf "%.0f", sum/NR}' /tmp/mem_history.txt);    MEM_MAX_PCT=$(awk "BEGIN {printf \"%.1f\", ($MEM_MAX/$MEM_TOTAL)*100}");   MEM_AVG_PCT=$(awk "BEGIN {printf \"%.1f\", ($MEM_AVG/$MEM_TOTAL)*100}");    echo "Router Status @ $(date +'%H:%M:%S')";   echo "🌡️  Temp: ${TEMP_C}°C";   echo "⚙️  CPU Load: ${CPU_LOAD}";   echo "🧠  RAM:";   echo "   - Max: ${MEM_MAX}MiB (${MEM_MAX_PCT}%)";   echo "   - Avg: ${MEM_AVG}MiB (${MEM_AVG_PCT}%)";   echo "   - Current: ${MEM_USED}MiB (${MEM_PERCENT}%)";    sleep $INTERVAL; done
#!/usr/bin/env bash
# mikrotik-live-snmp.sh
# Folosește SNMP pe RouterOS pentru Temp, RAM (Max/Avg/Current) și CPU.
# Configurabile:
ROUTER="192.168.10.1"
COMMUNITY="public"
INDEX=65536                # index pentru "main memory" (detectat pe dispozitivul tău)
STATE_FILE="/tmp/router_ram_kb.log"
REFRESH=5                  # secunde între refresh (schimbă dacă vrei)
HISTORY_MAX=300            # păstrează ultimele N măsurători
# verificări minimale
command -v snmpget >/dev/null 2>&1 || { echo "snmpget lipsește. Instalează pachetul snmp."; exit 1; }
command -v snmpwalk >/dev/null 2>&1 || { echo "snmpwalk lipsește. Instalează pachetul snmp."; exit 1; }
mkdir -p "$(dirname "$STATE_FILE")"
touch "$STATE_FILE"
while true; do   clear
  USED_RAW=$(snmpget -v2c -c "$COMMUNITY" "$ROUTER" HOST-RESOURCES-MIB::hrStorageUsed."$INDEX" 2>/dev/null | awk -F': ' '{print $NF}');   TOTAL_RAW=$(snmpget -v2c -c "$COMMUNITY" "$ROUTER" HOST-RESOURCES-MIB::hrStorageSize."$INDEX" 2>/dev/null | awk -F': ' '{print $NF}');   TEMP_RAW=$(snmpget -v2c -c "$COMMUNITY" -m +MIKROTIK-MIB "$ROUTER" mtxrGaugeValue.17 2>/dev/null | awk -F': ' '{print $NF}');   CPU_AVG=$(snmpwalk -v2c -c "$COMMUNITY" "$ROUTER" hrProcessorLoad 2>/dev/null | awk '{sum+=$NF; n++} END {if(n>0) printf "%.2f", sum/n; else print "0.00"}'); 
  if ! [[ "$USED_RAW" =~ ^[0-9]+$ ]] || ! [[ "$TOTAL_RAW" =~ ^[0-9]+$ ]]; then     echo "Eroare: nu am primit valori RAM de la $ROUTER (verifică SNMP / community / index).";     echo "Partial data: USED='$USED_RAW' TOTAL='$TOTAL_RAW' TEMP='$TEMP_RAW'";     sleep $REFRESH;     continue;   fi;    USED=$USED_RAW;   TOTAL=$TOTAL_RAW;   TEMP=${TEMP_RAW:-N/A}; 
  printf "%s\n" "$USED" >> "$STATE_FILE"; 
  tail -n "$HISTORY_MAX" "$STATE_FILE" > "${STATE_FILE}.tmp" && mv "${STATE_FILE}.tmp" "$STATE_FILE"; 
  MAX_USED=$(awk 'BEGIN{m=0}{if($1>m)m=$1}END{print (m+0)}' "$STATE_FILE");   AVG_USED=$(awk '{sum+=$1; n++} END {if(n>0) printf "%.0f", sum/n; else print 0}' "$STATE_FILE"); 
  USED_MiB=$(awk -v u="$USED" 'BEGIN{printf "%.2f", u/1024}');   MAX_MiB=$(awk -v u="$MAX_USED" 'BEGIN{printf "%.2f", u/1024}');   AVG_MiB=$(awk -v u="$AVG_USED" 'BEGIN{printf "%.2f", u/1024}'); 
  PCT_CUR=$(awk -v u="$USED" -v t="$TOTAL" 'BEGIN{printf "%.1f", (u/t)*100}');   PCT_MAX=$(awk -v u="$MAX_USED" -v t="$TOTAL" 'BEGIN{printf "%.1f", (u/t)*100}');   PCT_AVG=$(awk -v u="$AVG_USED" -v t="$TOTAL" 'BEGIN{printf "%.1f", (u/t)*100}'); 
  echo "Router Status @ $(date +%H:%M:%S)";   echo "🌡️  Temp: ${TEMP}°C";   echo "🧠  RAM: Max: ${MAX_MiB}MiB (${PCT_MAX}%); Average: ${AVG_MiB}MiB (${PCT_AVG}%); Current: ${USED_MiB}MiB (${PCT_CUR}%);";   echo "⚙️  CPU Avg Load: ${CPU_AVG}%";    sleep "$REFRESH"; done
