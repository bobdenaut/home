#!/bin/bash
# Monitor live ARC, dirty pages și I/O ZFS
echo "=== ZFS Desktop Live Monitor ==="
echo "Press Ctrl+C to exit."
echo ""
while true; do     date +"%F %T";     echo "---- ARC Stats ----";     cat /proc/spl/kstat/zfs/arcstats | awk '
    /size/ {print "ARC size (bytes): "$3}
    /c/ {print "ARC current size: "$3}
    /hits/ {hits=$3}
    /misses/ {misses=$3; printf "ARC hit rate: %.2f %%\n", (hits/(hits+misses))*100}';          echo "---- Dirty Pages ----";     echo -n "dirty_ratio: "; cat /proc/sys/vm/dirty_ratio;     echo -n "dirty_background_ratio: "; cat /proc/sys/vm/dirty_background_ratio;     echo -n "dirty_expire_centisecs: "; cat /proc/sys/vm/dirty_expire_centisecs;     echo -n "dirty_writeback_centisecs: "; cat /proc/sys/vm/dirty_writeback_centisecs;      echo "---- ZFS I/O Stats (last 2 sec) ----";     zpool iostat -v 2 1;      echo "";     sleep 1; done
exit
#!/bin/bash
# ZFS + ARC Live Monitor cu grafic în terminal
