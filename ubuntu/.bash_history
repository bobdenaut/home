#!/bin/bash
# Monitorizare trafic network live cu grafice ASCII
INTERFACES=$(ls /sys/class/net | grep -E 'enp|wlo')  # interfețe de interes
SLEEP_INTERVAL=1
MAX_BAR=50  # lungimea maximă a barei
declare -A RX_PREV
declare -A TX_PREV
# Initialize
for IF in $INTERFACES; do     RX_PREV[$IF]=$(cat /sys/class/net/$IF/statistics/rx_bytes);     TX_PREV[$IF]=$(cat /sys/class/net/$IF/statistics/tx_bytes); done
clear
echo "Monitoring interfaces: $INTERFACES"
echo "Press Ctrl+C to stop."
